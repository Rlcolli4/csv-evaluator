{
  "name": "csv-evaluator",
  "version": "0.1.0",
  "description": "A library to validate Excel and CSV files before uploading to a server, checking headers and SQL-injection risks",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "serve": "npx serve .",
    "prepublishOnly": "npm run build",
    "prepublishValidate": "npm pack --dry-run"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "maxWorkers": 1,
    "testMatch": ["**/*.test.ts"],
    "moduleNameMapper": {
      "^(.*)\\.js$": "$1"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.test.ts",
      "!src/types.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov"],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "keywords": [
    "csv",
    "excel",
    "validation",
    "file-upload",
    "sql-injection",
    "parser"
  ],
  "author": "Robert L Collins Jr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rlcolli4/csv-evaluator.git"
  },
  "homepage": "https://github.com/Rlcolli4/csv-evaluator#readme",
  "bugs": {
    "url": "https://github.com/Rlcolli4/csv-evaluator/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "papaparse": "^5.4.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.14",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**/*"
  ]
}
